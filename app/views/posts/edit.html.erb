<h1>Edit Post GRABEH!!</h1>

<%= form_with model: @post do |form| %>
        <div>
                <%= form.label :title %>
                <%= form.text_field :title %>
        </div>
        <div>
                <%= form.label :content %>
                <%= form.text_field :content %>
        </div>

        <div>
                <%= form.submit "Edit Post" %>
        </div>
<% end %>